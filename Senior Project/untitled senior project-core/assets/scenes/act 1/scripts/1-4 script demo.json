{
"cast": ["Duncan", "Malcolm", "Donalbain",
		 "Lennox", "Macbeth", "Banquo"],

"script":
	[
	 	{
	 		"lineID": 1,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-The sin of my ingratitude even now"}]
	 	},
	 	{
	 		"lineID": 2,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Was heavy on me. Thou art so far before"}]
	 	},
	 	{
	 		"lineID": 3,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-That swiftest wing of recompense is slow"}]
	 	},
	 	{
	 		"lineID": 4,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-To overtake thee. Would thou hadst less deserved,"}]
	 	},
	 	{
	 		"lineID": 5,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-That the proportion both of thanks and payment"}]
	 	},
	 	{
	 		"lineID": 6,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Might have been mine! Only I have left to say,"}]
	 	},
	 	{
			"lineID": 7,
			"sharedLine": false,
			"line": [{"actor": "Duncan", "text": "-More is thy due than more than all can pay."}]
		},
	 	{
	 		"lineID": 8,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-The service and the loyalty I owe,"}]
	 	},
	 	{
	 		"lineID": 9,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-In doing it, pays itself. Your Highness' part"}]
	 	},
	 	{
	 		"lineID": 10,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-Is to receive our duties."}]
	 	},
	 	{
	 		"lineID": 11,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-I have begun to plant thee, and will labor"}]
	 	},
	 	{
	 		"lineID": 12,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-To make thee full of growing."}]
	 	},
	 	{
	 		"lineID": 13,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Sons, kinsmen, thanes,"}]
	 	},
	 	{
	 		"lineID": 14,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-And you whose places are the nearest, know,"}]
	 	},
	 	{
	 		"lineID": 15,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-We will establish our estate upon"}]
	 	},
	 	{
	 		"lineID": 16,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Our eldest, Malcolm, whom we name hereafter"}]
	 	},
	 	{
	 		"lineID": 17,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-The Prince of Cumberland. Which honor must"}]
	 	},
	 	{
	 		"lineID": 40,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Not unaccompanied invest him only,"}]
	 	},
	 	{
			"lineID": 41,
			"sharedLine": false,
			"line": [{"actor": "Duncan", "text": "-But signs of nobleness, like stars, shall shine"}]
		},
	 	{
	 		"lineID": 42,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-From hence to Inverness,"}]
	 	},
	 	{
	 		"lineID": 43,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-And bind us further to you."}]
	 	},
	 	{
	 		"lineID": 45,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-I'll be myself the harbinger, and make joyful"}]
	 	},
	 	{
	 		"lineID": 46,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-The hearing of my wife with your approach,"}]
	 	},
	 	{
	 		"lineID": 47,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "-So humbly take my leave."},
	 				 {"actor": "Duncan", "text": "My worthy Cawdor!"}]
	 	},
	 	{
	 		"lineID": 48,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-The Prince of Cumberland! That is a step"}]
	 	},
	 	{
	 		"lineID": 49,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-On which I must fall down or else o'erleap,"}]
	 	},
	 	{
	 		"lineID": 50,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-For in my way it lies. Stars, hide your fires,"}]
	 	},
	 	{
			"lineID": 51,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "-Let not light see my black and deep desires."}]
		},
	 	{
	 		"lineID": 52,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-The eye wink at the hand, yet let that be"}]
	 	},
	 	{
	 		"lineID": 53,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "-Which the eye fears, when it is done, to see."}]
	 	},
	 	{
	 		"lineID": 54,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-True, worthy Banquo, he is full so valiant,"}]
	 	},
	 	{
	 		"lineID": 55,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-And in his commendations I am fed,"}]
	 	},
	 	{
	 		"lineID": 56,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-It is a banquet to me. Let's after him,"}]
	 	},
	 	{
	 		"lineID": 57,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Whose care is gone before to bid us welcome."}]
	 	},
	 	{
	 		"lineID": 58,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-It is a peerless kinsman."}]
	 	}]
}