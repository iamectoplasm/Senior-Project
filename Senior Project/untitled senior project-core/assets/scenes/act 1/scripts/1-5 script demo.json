{
"sceneIntro":
	{
		"title": "Act 1 Scene 5",
		"backgroundImage": "barley-hall-study-interior",
		"setting": "Macbeth's castle, Inverness",
		"description": [""]
	},

"script":
	[
	 	{
	 		"lineID": 1,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Great Glamis! Worthy Cawdor!"}]
	 	},
	 	{
	 		"lineID": 2,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Greater than both, by the all-hail hereafter!"}]
	 	},
	 	{
	 		"lineID": 3,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Thy letters have transported me beyond"}]
	 	},
	 	{
	 		"lineID": 4,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-This ignorant present, and I feel now"}]
	 	},
	 	{
	 		"lineID": 5,
	 		"sharedLine": true,
	 		"line": [{"actor": "Lady Macbeth", "text": "-The future in the instant."},
	 				 {"actor": "Macbeth", "text": "My dearest love,"}]
	 	},
	 	{
	 		"lineID": 6,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "-Duncan comes here tonight."},
	 				 {"actor": "Lady Macbeth", "text": "And when goes hence?"}]
	 	},
	 	{
			"lineID": 7,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "-Tomorrow, as he purposes."},
					 {"actor": "Lady Macbeth", "text": "Oh, never"}]
		},
	 	{
	 		"lineID": 8,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Shall sun that morrow see!"}]
	 	},
	 	{
	 		"lineID": 9,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Your face, my Thane, is as a book where men"}]
	 	},
	 	{
	 		"lineID": 10,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-May read strange matters. To beguile the time,"}]
	 	},
	 	{
	 		"lineID": 11,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Look like the time, bear welcome in your eye,"}]
	 	},
	 	{
	 		"lineID": 12,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Your hand, your tongue. Look like the innocent flower"}]
	 	},
	 	{
	 		"lineID": 13,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-But be the serpent under 't."}]
	 	},
	 	{
	 		"lineID": 14,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "-We will speak further."},
	 				 {"actor": "Lady Macbeth", "text": "Only look up clear."}]
	 	},
	 	{
	 		"lineID": 15,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-To alter favor ever is to fear."}]
	 	},
	 	{
	 		"lineID": 16,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "-Leave all the rest to me."}]
	 	}]
}