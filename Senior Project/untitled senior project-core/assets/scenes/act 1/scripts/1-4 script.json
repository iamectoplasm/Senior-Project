{
"sceneIntro":
	{
		"title": "Act 1 Scene 4",
		"backgroundImage": "heath-exterior",
		"setting": "Under construction",
		"description": ["Under construction"]
	},

"script":
	[
		{
			"lineID": 1,
			"sharedLine": false,
			"line": [{"actor": "Duncan", "text": "\u2022 Is the execution done on Cawdor? Are not"}]
		},
	 	{
	 		"lineID": 2,
	 		"sharedLine": true,
	 		"line": [{"actor": "Duncan", "text": "\u2022 Those in commission yet returned?"},
	 				 {"actor": "Malcolm", "text": "My liege,"}]
	 	},
	 	{
	 		"lineID": 3,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 They are not yet come back. But I have spoke"}]
	 	},
	 	{
	 		"lineID": 4,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 With one that saw him die, who did report"}]
	 	},
	 	{
	 		"lineID": 5,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 That very frankly he confessed his treasons,"}]
	 	},
	 	{
	 		"lineID": 6,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 Implored your Highness' pardon, and set forth"}]
	 	},
	 	{
	 		"lineID": 7,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 A deep repentance. Nothing in his life"}]
	 	},
	 	{
	 		"lineID": 8,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 Became him like his leaving it. He died"}]
	 	},
	 	{
	 		"lineID": 9,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 As one that had been studied in his death"}]
	 	},
	 	{
	 		"lineID": 10,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "\u2022 To throw away the dearest thing he owed"}]
	 	},
	 	{
			"lineID": 11,
			"sharedLine": true,
			"line": [{"actor": "Malcolm", "text": "\u2022 As 'twere a careless trifle."},
					 {"actor": "Duncan", "text": "There's no art"}]
		},
	 	{
	 		"lineID": 12,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 To find the mind's construction in the face."}]
	 	},
	 	{
	 		"lineID": 13,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 He was a gentleman on whom I built"}]
	 	},
	 	{
	 		"lineID": 14,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 An absolute trust. O worthiest Cousin!"}]
	 	},
	 	{
	 		"lineID": 15,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 The sin of my ingratitude even now"}]
	 	},
	 	{
	 		"lineID": 16,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 Was heavy on me. Thou art so far before"}]
	 	},
	 	{
	 		"lineID": 17,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 That swiftest wing of recompense is slow"}]
	 	},
	 	{
	 		"lineID": 18,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 To overtake thee. Would thou hadst less deserved,"}]
	 	},
	 	{
	 		"lineID": 19,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 That the proportion both of thanks and payment"}]
	 	},
	 	{
	 		"lineID": 20,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 Might have been mine! Only I have left to say,"}]
	 	},
	 	{
			"lineID": 21,
			"sharedLine": false,
			"line": [{"actor": "Duncan", "text": "\u2022 More is thy due than more than all can pay."}]
		},
	 	{
	 		"lineID": 22,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The service and the loyalty I owe,"}]
	 	},
	 	{
	 		"lineID": 23,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 In doing it, pays itself. Your Highness' part"}]
	 	},
	 	{
	 		"lineID": 24,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Is to receive our duties. And our duties"}]
	 	},
	 	{
	 		"lineID": 25,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Are to your throne and state children and servants,"}]
	 	},
	 	{
	 		"lineID": 26,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Which do but what they should, by doing everything"}]
	 	},
	 	{
	 		"lineID": 27,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Safe toward your love and honor."},
	 				 {"actor": "Duncan", "text": "Welcome hither."}]
	 	},
	 	{
	 		"lineID": 28,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 I have begun to plant thee, and will labor"}]
	 	},
	 	{
	 		"lineID": 29,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 To make thee full of growing. Noble Banquo,"}]
	 	},
	 	{
	 		"lineID": 30,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 That hast not less deserved, nor must be known"}]
	 	},
	 	{
			"lineID": 31,
			"sharedLine": false,
			"line": [{"actor": "Duncan", "text": "\u2022 No less to have done so, let me infold thee"}]
		},
	 	{
	 		"lineID": 32,
	 		"sharedLine": true,
	 		"line": [{"actor": "Duncan", "text": "\u2022 And hold thee to my heart."},
	 				 {"actor": "Banquo", "text": "\u2022 There if I grow,"}]
	 	},
	 	{
	 		"lineID": 33,
	 		"sharedLine": true,
	 		"line": [{"actor": "Banquo", "text": "\u2022 The harvest is your own."},
	 				 {"actor": "Duncan", "text": "\u2022 My plenteous joys,"}]
	 	},
	 	{
	 		"lineID": 34,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 Wanton in fullness, seek to hide themselves"}]
	 	},
	 	{
	 		"lineID": 35,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 In drops of sorrow. Sons, kinsmen, thanes,"}]
	 	},
	 	{
	 		"lineID": 36,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 And you whose places are the nearest, know,"}]
	 	},
	 	{
	 		"lineID": 37,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 We will establish our estate upon"}]
	 	},
	 	{
	 		"lineID": 38,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 Our eldest, Malcolm, whom we name hereafter"}]
	 	},
	 	{
	 		"lineID": 39,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 The Prince of Cumberland. Which honor must"}]
	 	},
	 	{
	 		"lineID": 40,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 Not unaccompanied invest him only,"}]
	 	},
	 	{
			"lineID": 41,
			"sharedLine": false,
			"line": [{"actor": "Duncan", "text": "\u2022 But signs of nobleness, like stars, shall shine"}]
		},
	 	{
	 		"lineID": 42,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 On all observers. From hence to Inverness,"}]
	 	},
	 	{
	 		"lineID": 43,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 And bind us further to you."}]
	 	},
	 	{
	 		"lineID": 44,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The rest is labor, which is not used for you."}]
	 	},
	 	{
	 		"lineID": 45,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 I'll be myself the harbinger, and make joyful"}]
	 	},
	 	{
	 		"lineID": 46,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The hearing of my wife with your approach,"}]
	 	},
	 	{
	 		"lineID": 47,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 So humbly take my leave."},
	 				 {"actor": "Duncan", "text": "My worthy Cawdor!"}]
	 	},
	 	{
	 		"lineID": 48,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The Prince of Cumberland! That is a step"}]
	 	},
	 	{
	 		"lineID": 49,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 On which I must fall down or else o'erleap,"}]
	 	},
	 	{
	 		"lineID": 50,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 For in my way it lies. Stars, hide your fires,"}]
	 	},
	 	{
			"lineID": 51,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Let not light see my black and deep desires."}]
		},
	 	{
	 		"lineID": 52,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The eye wink at the hand, yet let that be"}]
	 	},
	 	{
	 		"lineID": 53,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Which the eye fears, when it is done, to see."}]
	 	},
	 	{
	 		"lineID": 54,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 True, worthy Banquo, he is full so valiant,"}]
	 	},
	 	{
	 		"lineID": 55,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 And in his commendations I am fed,"}]
	 	},
	 	{
	 		"lineID": 56,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 It is a banquet to me. Let's after him,"}]
	 	},
	 	{
	 		"lineID": 57,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 Whose care is gone before to bid us welcome."}]
	 	},
	 	{
	 		"lineID": 58,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "\u2022 It is a peerless kinsman."}]
	 	}]
}