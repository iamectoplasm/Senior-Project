{
"sceneIntro":
	{
		"title": "Act 1 Scene 3",
		"backgroundImage": "dark-heath-exterior",
		"setting": "A wasteland. Thunder rumbles in the distance.",
		"description": ["Having won in their battle against Irish and Norwegian raiders- and Scottish rebels-",
		"Macbeth and Banquo make their ways home... but not before stumbling into a very strange encounter,",
		"with some very strange people."]
	},

"script":
	[
	//------------------------------------------------------------
	//			LINES 1 - 5
	//------------------------------------------------------------
		{
			"lineID": 1,
			"sharedLine": false,
			"line": [{"actor": "First Witch", "text": "\u2022 Where hast thou been, Sister?"}]
		},
	 	{
	 		"lineID": 2,
	 		"sharedLine": false,
	 		"line": [{"actor": "Second Witch", "text": "\u2022 Killing swine."}]
	 	},
	 	{
	 		"lineID": 3,
	 		"sharedLine": false,
	 		"line": [{"actor": "Third Witch", "text": "\u2022 Sister, where thou?"}]
	 	},
	 	{
	 		"lineID": 4,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 A sailor's wife had chestnuts in her lap,"}]
	 	},
	 	{
	 		"lineID": 5,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 And mounched, and mounched, and mounched. 'Give me,' quoth I."}]
	 	},
	//------------------------------------------------------------
	//			LINES 6 - 10
	//------------------------------------------------------------
	 	{
	 		"lineID": 6,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 'Aroint thee, witch!' the rump-fed ronyon cries."}]
	 	},
	 	{
	 		"lineID": 7,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Her husband's to Aleppo gone, master o' the Tiger."}]
	 	},
	 	{
	 		"lineID": 8,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 But in a sieve I'll thither sail"}]
	 	},
	 	{
	 		"lineID": 9,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 And, like a rat without a tail,"}]
	 	},
	 	{
	 		"lineID": 10,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 I'll do, I'll do, and I'll do."}]
	 	},
	//------------------------------------------------------------
	//			LINES 11 - 15
	//------------------------------------------------------------
	 	{
			"lineID": 11,
			"sharedLine": false,
			"line": [{"actor": "Second Witch", "text": "\u2022 I'll give thee a wind"}]
		},
	 	{
	 		"lineID": 12,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Thou'rt kind."}]
	 	},
	 	{
	 		"lineID": 13,
	 		"sharedLine": false,
	 		"line": [{"actor": "Third Witch", "text": "\u2022 And I another."}]
	 	},
	 	{
	 		"lineID": 14,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 I myself have all the other,"}]
	 	},
	 	{
	 		"lineID": 15,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 And the very ports they blow,"}]
	 	},
	//------------------------------------------------------------
	//			LINES 16 - 20
	//------------------------------------------------------------
	 	{
	 		"lineID": 16,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 All the quarters that they know"}]
	 	},
	 	{
	 		"lineID": 17,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 I' the shipman's card."}]
	 	},
	 	{
	 		"lineID": 18,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 I will drain him dry as hay."}]
	 	},
	 	{
	 		"lineID": 19,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Sleep shall neither night nor day"}]
	 	},
	 	{
	 		"lineID": 20,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Hang upon his penthouse lid."}]
	 	},
	//------------------------------------------------------------
	//			LINES 21 - 25
	//------------------------------------------------------------
	 	{
			"lineID": 21,
			"sharedLine": false,
			"line": [{"actor": "First Witch", "text": "\u2022 He shall live a man forbid."}]
		},
	 	{
	 		"lineID": 22,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Weary sennights nine times nine"}]
	 	},
	 	{
	 		"lineID": 23,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Shall he dwindle, peak, and pine."}]
	 	},
	 	{
	 		"lineID": 24,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Though his bark cannot be lost,"}]
	 	},
	 	{
	 		"lineID": 25,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Yet it shall be tempest-tost."}]
	 	},
	//------------------------------------------------------------
	//			LINES 26 - 30
	//------------------------------------------------------------
	 	{
	 		"lineID": 26,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Look what I have."}]
	 	},
	 	{
	 		"lineID": 27,
	 		"sharedLine": false,
	 		"line": [{"actor": "Second Witch", "text": "\u2022 Show me, show me."}]
	 	},
	 	{
	 		"lineID": 28,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Here I have a pilot's thumb,"}]
	 	},
	 	{
	 		"lineID": 29,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Wrecked as homeward he did come."}]
	 	},
	 	{
	 		"lineID": 30,
	 		"sharedLine": false,
	 		"line": [{"actor": "Third Witch", "text": "\u2022 A drum, a drum!"}]
	 	},
	//------------------------------------------------------------
	//			LINES 31 - 35
	//------------------------------------------------------------
	 	{
			"lineID": 31,
			"sharedLine": false,
			"line": [{"actor": "Third Witch", "text": "\u2022 Macbeth doth come."}]
		},
	 	{
	 		"lineID": 32,
	 		"sharedLine": false,
	 		"line": [{"actor": "All", "text": "\u2022 The weird sisters, hand in hand,"}]
	 	},
	 	{
	 		"lineID": 33,
	 		"sharedLine": false,
	 		"line": [{"actor": "All", "text": "\u2022 Posters of the sea and land,"}]
	 	},
	 	{
	 		"lineID": 34,
	 		"sharedLine": false,
	 		"line": [{"actor": "All", "text": "\u2022 Thus do go about, about."}]
	 	},
	 	{
	 		"lineID": 35,
	 		"sharedLine": false,
	 		"line": [{"actor": "All", "text": "\u2022 Thrice to thine,and thrice to mine,"}]
	 	},
	//------------------------------------------------------------
	//			LINES 36 - 40
	//------------------------------------------------------------
	 	{
	 		"lineID": 36,
	 		"sharedLine": false,
	 		"line": [{"actor": "All", "text": "\u2022 And thrice again, to make up nine,"}]
	 	},
	 	{
	 		"lineID": 37,
	 		"sharedLine": false,
	 		"line": [{"actor": "All", "text": "\u2022 Peace! The charm's wound up."}]
	 	},
	 	{
	 		"lineID": 38,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 So foul and fair a day I have not seen."}]
	 	},
	 	{
	 		"lineID": 39,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 How far is 't called to Forres? What are these"}]
	 	},
	 	{
	 		"lineID": 40,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 So withered, and so wild in their attire,"}]
	 	},
	//------------------------------------------------------------
	//			LINES 41 - 45
	//------------------------------------------------------------
	 	{
			"lineID": 41,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 That look not like the inhabitants o' the earth"}]
		},
	 	{
	 		"lineID": 42,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 And yet are on 't? Live you? Or are you aught"}]
	 	},
	 	{
	 		"lineID": 43,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 That man may question? You seem to understand me,"}]
	 	},
	 	{
	 		"lineID": 44,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 By each at once her choppy finger laying"}]
	 	},
	 	{
	 		"lineID": 45,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Upon her skinny lips. You should be women,"}]
	 	},
	//------------------------------------------------------------
	//			LINES 46 - 50
	//------------------------------------------------------------
	 	{
	 		"lineID": 46,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 And yet your beards forbid me to interpret"}]
	 	},
	 	{
	 		"lineID": 47,
	 		"sharedLine": true,
	 		"line": [{"actor": "Banquo", "text": "\u2022 That you are so."},
	 				 {"actor": "Macbeth", "text": "Speak, if you can. What are you?"}]
	 	},
	 	{
	 		"lineID": 48,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 All hail, Macbeth! Hail to thee, Thane of Glamis!"}]
	 	},
	 	{
	 		"lineID": 49,
	 		"sharedLine": false,
	 		"line": [{"actor": "Second Witch", "text": "\u2022 All hail, Macbeth! Hail to thee, Thane of Cawdor!"}]
	 	},
	 	{
	 		"lineID": 50,
	 		"sharedLine": false,
	 		"line": [{"actor": "Third Witch", "text": "\u2022 All hail, Macbeth, that shalt be King hereafter!"}]
	 	},
	//------------------------------------------------------------
	//			LINES 51 - 55
	//------------------------------------------------------------
	 	{
			"lineID": 51,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 Good sir, why do you start, and seem to fear"}]
		},
	 	{
	 		"lineID": 52,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Things that do sound so fair? I' the name of truth,"}]
	 	},
	 	{
	 		"lineID": 53,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Are ye fantastical, or that indeed"}]
	 	},
	 	{
	 		"lineID": 54,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Which outwardly ye show? My noble partner"}]
	 	},
	 	{
	 		"lineID": 55,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 You greet with present grace and great prediction"}]
	 	},
	//------------------------------------------------------------
	//			LINES 56 - 60
	//------------------------------------------------------------
	 	{
	 		"lineID": 56,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Of noble having and of royal hope,"}]
	 	},
	 	{
	 		"lineID": 57,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 That he seems rapt withal. To me you speak not."}]
	 	},
	 	{
	 		"lineID": 58,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 If you can look into the seeds of time"}]
	 	},
	 	{
	 		"lineID": 59,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 And say which grain will grow and which will not,"}]
	 	},
	 	{
	 		"lineID": 60,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Speak then to me, who neither beg nor fear"}]
	 	},
	//------------------------------------------------------------
	//			LINES 61 - 65
	//------------------------------------------------------------
	 	{
			"lineID": 61,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 Your favors nor your hate."}]
		},
	 	{
	 		"lineID": 62,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Hail!"}]
	 	},
	 	{
	 		"lineID": 63,
	 		"sharedLine": false,
	 		"line": [{"actor": "Second Witch", "text": "\u2022 Hail!"}]
	 	},
	 	{
	 		"lineID": 64,
	 		"sharedLine": false,
	 		"line": [{"actor": "Third Witch", "text": "\u2022 Hail!"}]
	 	},
	 	{
	 		"lineID": 65,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Lesser than Macbeth, and greater."}]
	 	},
	//------------------------------------------------------------
	//			LINES 66 - 70
	//------------------------------------------------------------
	 	{
	 		"lineID": 66,
	 		"sharedLine": false,
	 		"line": [{"actor": "Second witch", "text": "\u2022 Not so happy, yet much happier."}]
	 	},
	 	{
	 		"lineID": 67,
	 		"sharedLine": false,
	 		"line": [{"actor": "Third Witch", "text": "\u2022 Thou shalt get kings, though thou be none."}]
	 	},
	 	{
	 		"lineID": 68,
	 		"sharedLine": false,
	 		"line": [{"actor": "Third Witch", "text": "\u2022 So all hail, Macbeth and Banquo!"}]
	 	},
	 	{
	 		"lineID": 69,
	 		"sharedLine": false,
	 		"line": [{"actor": "First Witch", "text": "\u2022 Banquo and Macbeth, all hail!"}]
	 	},
	 	{
	 		"lineID": 70,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Stay, you imperfect speakers, tell me more."}]
	 	},
	//------------------------------------------------------------
	//			LINES 71 - 75
	//------------------------------------------------------------
	 	{
			"lineID": 71,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 By Sinel's death I know I am Thane of Glamis,"}]
		},
	 	{
	 		"lineID": 72,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 But how of Cawdor? The Thane of Cawdor lives,"}]
	 	},
	 	{
	 		"lineID": 73,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 A prosperous gentleman, and to be King"}]
	 	},
	 	{
	 		"lineID": 74,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Stands not within the prospect of belief,"}]
	 	},
	 	{
	 		"lineID": 75,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 No more than to be Cawdor. Say from whence"}]
	 	},
	//------------------------------------------------------------
	//			LINES 76 - 80
	//------------------------------------------------------------
	 	{
	 		"lineID": 76,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 You owe this strange intelligence? or why"}]
	 	},
	 	{
	 		"lineID": 77,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Upon this blasted heath you stop our way"}]
	 	},
	 	{
	 		"lineID": 78,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 With such prophetic greeting? Speak, I charge you."}]
	 	},
	 	{
	 		"lineID": 79,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 The earth hath bubbles as the water has,"}]
	 	},
	 	{
	 		"lineID": 80,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 And these are of them. Whither are they vanished?"}]
	 	},
	//------------------------------------------------------------
	//			LINES 81 - 85
	//------------------------------------------------------------
	 	{
			"lineID": 81,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Into the air; and what seemed corporal melted"}]
		},
	 	{
	 		"lineID": 82,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 As breath into the wind. Would they had stay'd!"}]
	 	},
	 	{
	 		"lineID": 83,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Were such things here as we do speak about?"}]
	 	},
	 	{
	 		"lineID": 84,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Or have we eaten on the insane root"}]
	 	},
	 	{
	 		"lineID": 85,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 That takes the reason prisoner?"}]
	 	},
	//------------------------------------------------------------
	//			LINES 86 - 90
	//------------------------------------------------------------
	 	{
	 		"lineID": 86,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Your children shall be kings."},
	 				 {"actor": "Banquo", "text": "You shall be King."}]
	 	},
	 	{
	 		"lineID": 87,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 And Thane of Cawdor too. Went it not so?"}]
	 	},
	 	{
	 		"lineID": 88,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 To the selfsame tune and words. Who's here?"}]
	 	},
	 	{
	 		"lineID": 89,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 The King hath happily received, Macbeth,"}]
	 	},
	 	{
	 		"lineID": 90,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 The news of thy success. And when he reads"}]
	 	},
	//------------------------------------------------------------
	//			LINES 91 - 95
	//------------------------------------------------------------
	 	{
			"lineID": 91,
			"sharedLine": false,
			"line": [{"actor": "Ross", "text": "\u2022 Thy personal venture in the rebels' fight,"}]
		},
	 	{
	 		"lineID": 92,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 His wonders and his praises do contend"}]
	 	},
	 	{
	 		"lineID": 93,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 Which should be thine or his. Silenced with that,"}]
	 	},
	 	{
	 		"lineID": 94,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 In viewing o'er the rest o' the selfsame day,"}]
	 	},
	 	{
	 		"lineID": 95,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 He finds thee in stout Norweyan ranks,"}]
	 	},
	//------------------------------------------------------------
	//			LINES 96 - 100
	//------------------------------------------------------------
	 	{
	 		"lineID": 96,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 Nothing afeared of what thyself didst make,"}]
	 	},
	 	{
	 		"lineID": 97,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 Strange images of death. As thick as hail"}]
	 	},
	 	{
	 		"lineID": 98,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 Came post with post, and every one did bear"}]
	 	},
	 	{
	 		"lineID": 99,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 Thy praises in his kingdom's great defense,"}]
	 	},
	 	{
	 		"lineID": 100,
	 		"sharedLine": true,
	 		"line": [{"actor": "Ross", "text": "\u2022 And poured them down before them."},
	 				 {"actor": "Angus", "text": "We are sent"}]
	 	},
	//------------------------------------------------------------
	//			LINES 101 - 105
	//------------------------------------------------------------
	 	{
			"lineID": 101,
			"sharedLine": false,
			"line": [{"actor": "Angus", "text": "\u2022 To give thee, from our royal master, thanks,"}]
		},
	 	{
	 		"lineID": 102,
	 		"sharedLine": false,
	 		"line": [{"actor": "Angus", "text": "\u2022 Only to herald thee into his sight,"}]
	 	},
	 	{
	 		"lineID": 103,
	 		"sharedLine": false,
	 		"line": [{"actor": "Angus", "text": "\u2022 Not pay thee."}]
	 	},
	 	{
	 		"lineID": 104,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 And for an earnest of a greater honor,"}]
	 	},
	 	{
	 		"lineID": 105,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 He bade me, from him, call thee Thane of Cawdor."}]
	 	},
	//------------------------------------------------------------
	//			LINES 106 - 110
	//------------------------------------------------------------
	 	{
	 		"lineID": 106,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "\u2022 In which addition, hail, most worthy Thane!"}]
	 	},
	 	{
	 		"lineID": 107,
	 		"sharedLine": true,
	 		"line": [{"actor": "Ross", "text": "\u2022 For it is thine."},
	 				 {"actor": "Banquo", "text": "What, can the Devil speak true?"}]
	 	},
	 	{
	 		"lineID": 108,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The Thane of Cawdor lives. Why do you dress me"}]
	 	},
	 	{
	 		"lineID": 109,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 In borrowed robes?"},
	 				 {"actor": "Angus", "text": "Who was the Thane lives yet,"}]
	 	},
	 	{
	 		"lineID": 110,
	 		"sharedLine": false,
	 		"line": [{"actor": "Angus", "text": "\u2022 But under heavy judgement bears that life"}]
	 	},
	//------------------------------------------------------------
	//			LINES 111 - 115
