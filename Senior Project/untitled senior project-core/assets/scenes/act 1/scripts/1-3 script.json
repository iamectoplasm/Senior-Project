	 	{
			"lineID": 111,
			"sharedLine": false,
			"line": [{"actor": "Angus", "text": "\u2022 Which he deserves to lose. Whether he was combined"}]
		},
	 	{
	 		"lineID": 112,
	 		"sharedLine": false,
	 		"line": [{"actor": "Angus", "text": "\u2022 With those of Norway, or did line the rebel"}]
	 	},
	 	{
	 		"lineID": 113,
	 		"sharedLine": false,
	 		"line": [{"actor": "Angus", "text": "\u2022 With hidden help and vantage, or that with both"}]
	 	},
	 	{
	 		"lineID": 114,
	 		"sharedLine": false,
	 		"line": [{"actor": "Angus", "text": "\u2022 He labored in his country's wreck, I know not."}]
	 	},
	 	{
	 		"lineID": 115,
	 		"sharedLine": false,
	 		"line": [{"actor": "Angus", "text": "\u2022 But treasons capital, confessed and proved,"}]
	 	},
	//------------------------------------------------------------
	//			LINES 116 - 120
	//------------------------------------------------------------
	 	{
	 		"lineID": 116,
	 		"sharedLine": true,
	 		"line": [{"actor": "Angus", "text": "\u2022 Have overthrown him."},
	 				 {"actor": "Macbeth", "text": "Glamis, and Thane of Cawdor."}]
	 	},
	 	{
	 		"lineID": 117,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The greatest is behind. \u2014Thanks for your pains.\u2014"}]
	 	},
	 	{
	 		"lineID": 118,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Do you not hope your children shall be kings,"}]
	 	},
	 	{
	 		"lineID": 119,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 When those that gave the Thane of Cawdor to me"}]
	 	},
	 	{
	 		"lineID": 120,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Promised no less to them?"},
	 				 {"actor": "Banquo", "text": "That, trusted home,"}]
	 	},
	//------------------------------------------------------------
	//			LINES 121 - 125
	//------------------------------------------------------------
	 	{
			"lineID": 121,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 Might yet enkindle you unto the crown,"}]
		},
	 	{
	 		"lineID": 122,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Besides the Thane of Cawdor. But 'tis strange."}]
	 	},
	 	{
	 		"lineID": 123,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 And oftentimes, to win us to our harm,"}]
	 	},
	 	{
	 		"lineID": 124,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 The instruments of darkness tell us truths,"}]
	 	},
	 	{
	 		"lineID": 125,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Win us with honest trifles, to betray 's"}]
	 	},
	//------------------------------------------------------------
	//			LINES 126 - 130
	//------------------------------------------------------------
	 	{
	 		"lineID": 126,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 In deepest consequence."}]
	 	},
	 	{
	 		"lineID": 127,
	 		"sharedLine": true,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Cousins, a word, I pray you"},
	 				 {"actor": "Macbeth", "text": "Two truths are told"}]
	 	},
	 	{
	 		"lineID": 128,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 As happy prologues to the swelling act"}]
	 	},
	 	{
	 		"lineID": 129,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Of the imperial theme. \u2014I thank you, gentlemen.\u2014"}]
	 	},
	 	{
	 		"lineID": 130,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 This supernatural soliciting"}]
	 	},
	//------------------------------------------------------------
	//			LINES 131 - 135
	//------------------------------------------------------------
	 	{
			"lineID": 131,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Cannot be ill, cannot be good. If ill,"}]
		},
	 	{
	 		"lineID": 132,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Why hath it given me earnest of success,"}]
	 	},
	 	{
	 		"lineID": 133,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Commencing in a truth? I am Thane of Cawdor."}]
	 	},
	 	{
	 		"lineID": 134,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 If good, why do I yield to that suggestion"}]
	 	},
	 	{
	 		"lineID": 135,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Whose horrid image doth unfix my hair"}]
	 	},
	//------------------------------------------------------------
	//			LINES 136 - 140
	//------------------------------------------------------------
	 	{
	 		"lineID": 136,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 And make my seated heart knock at my ribs,"}]
	 	},
	 	{
	 		"lineID": 137,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Against the use of nature? Present fears"}]
	 	},
	 	{
	 		"lineID": 138,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Are less than horrible imaginings."}]
	 	},
	 	{
	 		"lineID": 139,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 My thought, whose murder yet is but fantastical,"}]
	 	},
	 	{
	 		"lineID": 140,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Shakes so my single state of man that function"}]
	 	},
	//------------------------------------------------------------
	//			LINES 141 - 145
	//------------------------------------------------------------
	 	{
			"lineID": 141,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Is smothered in surmise, and nothing is"}]
		},
	 	{
	 		"lineID": 142,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 But what is not."},
	 				 {"actor": "Banquo", "text": "Look how our partner's rapt."}]
	 	},
	 	{
	 		"lineID": 143,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 If chance will have me King, why, chance may crown me,"}]
	 	},
	 	{
	 		"lineID": 144,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Without my stir."},
	 				 {"actor": "Banquo", "text": "New honors come upon him,"}]
	 	},
	 	{
	 		"lineID": 145,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Like our strange garments, cleave not to their mold"}]
	 	},
	//------------------------------------------------------------
	//			LINES 146 - 150
	//------------------------------------------------------------
	 	{
	 		"lineID": 146,
	 		"sharedLine": true,
	 		"line": [{"actor": "Banquo", "text": "\u2022 But with the aid of use."},
	 				 {"actor": "Macbeth", "text": "Come what may,"}]
	 	},
	 	{
	 		"lineID": 147,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Time and the hour runs through the roughest day."}]
	 	},
	 	{
	 		"lineID": 148,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Worthy Macbeth, we stay upon your leisure."}]
	 	},
	 	{
	 		"lineID": 149,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Give me your favor. My dull brain was wrought"}]
	 	},
	 	{
	 		"lineID": 150,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 With things forgotten. Kind gentlemen, your pains"}]
