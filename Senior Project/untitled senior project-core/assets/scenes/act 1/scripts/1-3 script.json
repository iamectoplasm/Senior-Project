	 	{
			"lineID": 151,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Are registered where every day I turn"}]
		},
	 	{
	 		"lineID": 152,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 The leaf to read them. Let us toward the King."}]
	 	},
	 	{
	 		"lineID": 153,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Think upon what hath chanced, and at more time,"}]
