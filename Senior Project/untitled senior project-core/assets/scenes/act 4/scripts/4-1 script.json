		{
			"lineID": 1,
			"sharedLine": false,
			"line": [{"actor": "First Witch", "text": "\u2022 Thrice the brinded cat hath mewed."}]
		},
	 	{
	 		"lineID": 2,
	 		"sharedLine": false,
	 		"line": [{"actor": "Second Witch", "text": "\u2022 Thrice and once the hedgepig whined."}]
