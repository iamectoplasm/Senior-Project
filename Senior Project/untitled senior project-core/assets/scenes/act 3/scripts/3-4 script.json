{
"sceneIntro":
	{
		"title": "Act 3 Scene 4",
		"backgroundImage": "heath-exterior",
		"setting": "Under construction",
		"description": ["Under construction"]
	},

"script":
	[
	//------------------------------------------------------------
	//				LINES 1 - 5
	//------------------------------------------------------------
		{
			"lineID": 1,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 You know your own degrees, sit down. At first"}]
		},
	 	{
	 		"lineID": 2,
	 		"sharedLine": true,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 And last the hearty welcome."},
	 				 {"actor": "Lords", "text": "Thanks to your majesty."}]
	 	},
	 	{
	 		"lineID": 3,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Ourself will mingle with society"}]
	 	},
	 	{
	 		"lineID": 4,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 And play the humble host."}]
	 	},
	 	{
	 		"lineID": 5,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Our hostess keeps her state, but in best time"}]
	 	},
	//------------------------------------------------------------
	//				LINES 6 - 10
	//------------------------------------------------------------
	 	{
	 		"lineID": 6,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 We will require her welcome."}]
	 	},
	 	{
	 		"lineID": 7,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "\u2022 Pronounce it for me, sir, to all our friends,"}]
	 	},
	 	{
	 		"lineID": 8,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lady Macbeth", "text": "\u2022 For my heart speaks they are welcome."}]
	 	},
	 	{
	 		"lineID": 9,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 See, they encounter thee with their hearts' thanks."}]
	 	},
	 	{
	 		"lineID": 10,
	 		"sharedLine": false,
	 		"line": [{"actor": "Macbeth", "text": "\u2022 Both sides are even. Here I'll sit i' the midst."}]
	 	},
	//------------------------------------------------------------
	//				LINES 11 - 15
	//------------------------------------------------------------
		{
			"lineID": 11,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Be large in mirth, anon we'll drink a measure"}]
		},
		{
			"lineID": 12,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 The table round. There's blood on thy face."}]
		},
		{
			"lineID": 13,
			"sharedLine": false,
			"line": [{"actor": "First Murderer", "text": "\u2022 'Tis Banquo's then."}]
		},
		{
			"lineID": 14,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 'Tis better thee without than he within."}]
		},
		{
			"lineID": 15,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Is he dispatched?"}]
		},
	//------------------------------------------------------------
	//				LINES 16 - 20
	//------------------------------------------------------------
		{
			"lineID": 16,
			"sharedLine": false,
			"line": [{"actor": "First Murderer", "text": "\u2022 My lord, his throat is cut. That I did for him."}]
		},
		{
			"lineID": 17,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thou art the best o' the cut-throats. Yet he's good"}]
		},
		{
			"lineID": 18,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 That did the like for Fleance. If thou didst it,"}]
		},
		{
			"lineID": 19,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thou art the nonpareil."},
	 				 {"actor": "First Murderer", "text": "Most royal sir,"}]
		},
		{
			"lineID": 20,
			"sharedLine": false,
			"line": [{"actor": "First Murderer", "text": "\u2022 Fleance is 'scaped."}]
		},
	//------------------------------------------------------------
	//				LINES 21 - 25
	//------------------------------------------------------------
		{
			"lineID": 21,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Then comes my fit again. I had else been perfect,"}]
		},
		{
			"lineID": 22,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Whole as the marble, founded as the rock,"}]
		},
		{
			"lineID": 23,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 As broad and general as the casing air."}]
		},
		{
			"lineID": 24,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 But now I am cabined, cribbed, confined, bound in"}]
		},
		{
			"lineID": 25,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 To saucy doubts and fears./u2014But Banquo's safe?"}]
		},
	//------------------------------------------------------------
	//				LINES 26 - 30
	//------------------------------------------------------------
		{
			"lineID": 26,
			"sharedLine": false,
			"line": [{"actor": "First Murderer", "text": "\u2022 Aye, my good lord. Safe in a ditch he bides,"}]
		},
		{
			"lineID": 27,
			"sharedLine": false,
			"line": [{"actor": "First Murderer", "text": "\u2022 With twenty trench\u00E8d gashes on his head,"}]
		},
		{
			"lineID": 28,
			"sharedLine": true,
			"line": [{"actor": "First Murderer", "text": "\u2022 The least a death to nature."},
	 				 {"actor": "Macbeth", "text": "Thanks for that."}]
		},
		{
			"lineID": 29,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 There the grown serpent lies. The worm that's fled"}]
		},
		{
			"lineID": 30,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Hath nature that in time will venom breed,"}]
		},
	//------------------------------------------------------------
	//				LINES 31 - 35
	//------------------------------------------------------------
		{
			"lineID": 31,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 No teeth for the present. Get thee gone. Tomorrow"}]
		},
		{
			"lineID": 32,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 We'll hear, ourselves, again."},
	 				 {"actor": "Lady Macbeth", "text": "My royal lord,"}]
		},
		{
			"lineID": 33,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 You do not give the cheer. The feast is sold"}]
		},
		{
			"lineID": 34,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 That is not often vouched, while 'tis a-making,"}]
		},
		{
			"lineID": 35,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 'Tis given with welcome. To feed were best at home,"}]
		},
	//------------------------------------------------------------
	//				LINES 36 - 40
	//------------------------------------------------------------
		{
			"lineID": 36,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 From thence the sauce to meat is ceremony,"}]
		},
		{
			"lineID": 37,
			"sharedLine": true,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Meeting were bare without it."},
	 				 {"actor": "Macbeth", "text": "Sweet remembrancer!"}]
		},
		{
			"lineID": 38,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Now good digestion wait on appetite,"}]
		},
		{
			"lineID": 39,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 And health on both!"},
	 				 {"actor": "Lennox", "text": "May 't please your Highness sit."}]
		},
		{
			"lineID": 40,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Here had we now our country's honor roofed"}]
		},
	//------------------------------------------------------------
	//				LINES 41 - 45
	//------------------------------------------------------------
		{
			"lineID": 41,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Were the graced person of our Banquo present,"}]
		},
		{
			"lineID": 42,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Who may I rather challenge for unkindness"}]
		},
		{
			"lineID": 43,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Than pity for mischance!"},
	 				 {"actor": "Ross", "text": "His absence, sir,"}]
		},
		{
			"lineID": 44,
			"sharedLine": false,
			"line": [{"actor": "Ross", "text": "\u2022 Lays blame upon his promise. Please 't your Highness"}]
		},
		{
			"lineID": 45,
			"sharedLine": false,
			"line": [{"actor": "Ross", "text": "\u2022 To grace us with your royal company."}]
		},
	//------------------------------------------------------------
	//				LINES 46 - 50
	//------------------------------------------------------------
		{
			"lineID": 46,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 The table's full."},
	 				 {"actor": "Lennox", "text": "Here is a place reserved, sir."}]
		},
		{
			"lineID": 47,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Where?"}]
		},
		{
			"lineID": 48,
			"sharedLine": false,
			"line": [{"actor": "Lennox", "text": "\u2022 Here, my good lord. What is 't that moves your Highness?"}]
		},
		{
			"lineID": 49,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Which of you have done this?"},
	 				 {"actor": "Lords", "text": "What, my good lord?"}]
		},
		{
			"lineID": 50,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thou canst not say I did it. Never shake"}]
		},
	//------------------------------------------------------------
	//				LINES 51 - 55
	//------------------------------------------------------------
		{
			"lineID": 51,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thy gory locks at me."}]
		},
		{
			"lineID": 52,
			"sharedLine": false,
			"line": [{"actor": "Ross", "text": "\u2022 Gentlemen, rise. His Highness is not well."}]
		},
		{
			"lineID": 53,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Sit, worthy friends. My lord is often thus,"}]
		},
		{
			"lineID": 54,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 And hath been from his youth. Pray you keep seat;"}]
		},
		{
			"lineID": 55,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 The fit is momentary, upon a thought"}]
		},
	//------------------------------------------------------------
	//				LINES 56 - 60
	//------------------------------------------------------------
		{
			"lineID": 56,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 He will again be well. If much you note him,"}]
		},
		{
			"lineID": 57,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 You shall offend him and extend his passion."}]
		},
		{
			"lineID": 58,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Feed, and regard him not. Are you a man?"}]
		},
		{
			"lineID": 59,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Aye, and a bold one, that dare look on that"}]
		},
		{
			"lineID": 60,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Which might appal the devil."},
	 				 {"actor": "Lady Macbeth", "text": "Oh, proper stuff!"}]
		},
	//------------------------------------------------------------
	//				LINES 61 - 65
	//------------------------------------------------------------
		{
			"lineID": 61,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 This is the very painting of your fear."}]
		},
		{
			"lineID": 62,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 This is the air-drawn dagger which, you said,"}]
		},
		{
			"lineID": 63,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Led you to Duncan. Oh, these flaws and starts,"}]
		},
		{
			"lineID": 64,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Impostors to true fear, would well become"}]
		},
		{
			"lineID": 65,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 A woman's story at a winter's fire,"}]
		},
	//------------------------------------------------------------
	//				LINES 66 - 70
	//------------------------------------------------------------
		{
			"lineID": 66,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Authorized by her grandam. Shame itself!"}]
		},
		{
			"lineID": 67,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Why do you make such faces? When all's done,"}]
		},
		{
			"lineID": 68,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 You look but on a stool."}]
		},
		{
			"lineID": 69,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Prithee, see there! Behold! Look! Lo! How say you?"}]
		},
		{
			"lineID": 70,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Why, what care I? If thou canst nod, speak too."}]
		},
	//------------------------------------------------------------
	//				LINES 71 - 75
	//------------------------------------------------------------
		{
			"lineID": 71,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 If charnel houses and our graves must send"}]
		},
		{
			"lineID": 72,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Those that we bury back, our monuments"}]
		},
		{
			"lineID": 73,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Shall be the maws of kites."},
	 				 {"actor": "Lady Macbeth", "text": "What, quite unmanned in folly?"}]
		},
		{
			"lineID": 74,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 If I stand here, I saw him."},
	 				 {"actor": "Lady Macbeth", "text": "Fie, for shame!"}]
		},
		{
			"lineID": 75,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Blood hath been shed ere now, i' the olden time,"}]
		},
	//------------------------------------------------------------
	//				LINES 76 - 80
	//------------------------------------------------------------
		{
			"lineID": 76,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Ere human statute purged the gentle weal\u2014"}]
		},
		{
			"lineID": 77,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Aye, and since too, murders have been performed"}]
		},
		{
			"lineID": 78,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Too terrible for the ear. The time has been"}]
		},
		{
			"lineID": 79,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 That when the brains were out, the man would die,"}]
		},
		{
			"lineID": 80,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And there an end. But now they rise again,"}]
		},
	//------------------------------------------------------------
	//				LINES 81 - 85
	//------------------------------------------------------------
		{
			"lineID": 81,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 With twenty mortal murders on their crowns,"}]
		},
		{
			"lineID": 82,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And push us from our stools. This is more strange"}]
		},
		{
			"lineID": 83,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Than such a murder is."},
	 				 {"actor": "Lady Macbeth", "text": "My worthy lord,"}]
		},
		{
			"lineID": 84,
			"sharedLine": true,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Your noble friends do lack you."},
	 				 {"actor": "Macbeth", "text": "I do forget."}]
		},
		{
			"lineID": 85,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Do not muse at me, my most worthy friends."}]
		},
	//------------------------------------------------------------
	//				LINES 86 - 90
	//------------------------------------------------------------
		{
			"lineID": 86,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I have a strange infirmity, which is nothing"}]
		},
		{
			"lineID": 87,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 To those that know me. Come, love and health to all,"}]
		},
		{
			"lineID": 88,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Then I'll sit down. Give me some wine, fill full."}]
		},
		{
			"lineID": 89,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I drink to the general joy o' the whole table,"}]
		},
		{
			"lineID": 90,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And to our dear friend Banquo, whom we miss."}]
		},
	//------------------------------------------------------------
	//				LINES 91 - 95
	//------------------------------------------------------------
		{
			"lineID": 91,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Would he were here! To all, and him, we thirst,"}]
		},
		{
			"lineID": 92,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 And all to all."},
	 				 {"actor": "Lords", "text": "Our duties, and the pledge."}]
		},
		{
			"lineID": 93,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Avaunt! And quit my sight! Let the earth hide thee!"}]
		},
		{
			"lineID": 94,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thy bones are marrowless, thy blood is cold,"}]
		},
		{
			"lineID": 95,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thou hast no speculation in those eyes"}]
		},
	//------------------------------------------------------------
	//				LINES 96 - 100
	//------------------------------------------------------------
		{
			"lineID": 96,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Which thou dost glare with!"},
	 				 {"actor": "Lady Macbeth", "text": "Think of this, good peers,"}]
		},
		{
			"lineID": 97,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 But as a thing of custom. 'Tis no other,"}]
		},
		{
			"lineID": 98,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Only it spoils the pleasure of the time."}]
		},
		{
			"lineID": 99,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 What man dare, I dare."}]
		},
		{
			"lineID": 100,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Approach thou like the rugged Russian bear,"}]
		},
	//------------------------------------------------------------
	//				LINES 101 - 105
	//------------------------------------------------------------
		{
			"lineID": 101,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 The armed rhinoceros, or the Hyrcan tiger,"}]
		},
		{
			"lineID": 102,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Take any shape but that and my firm nerves"}]
		},
		{
			"lineID": 103,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Shall never tremble. Or be alive again,"}]
		},
		{
			"lineID": 104,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And dare me to the desert with thy sword."}]
		},
		{
			"lineID": 105,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 If trembling I inhabit then, protest me"}]
		},
	//------------------------------------------------------------
	//				LINES 106 - 110
	//------------------------------------------------------------
		{
			"lineID": 106,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 The baby of a girl. Hence, horrible shadow!"}]
		},
		{
			"lineID": 107,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Unreal mockery, hence! Why, so. Being gone,"}]
		},
		{
			"lineID": 108,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I am a man again. Pray you, sit still."}]
		},
		{
			"lineID": 109,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 You have displaced the mirth, broke the good meeting,"}]
		},
		{
			"lineID": 110,
			"sharedLine": true,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 With most admired disorder."},
	 				 {"actor": "Macbeth", "text": "Can such things be,"}]
		},
	//------------------------------------------------------------
	//				LINES 111 - 115
	//------------------------------------------------------------
		{
			"lineID": 111,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And overcome us like a summer's cloud,"}]
		},
		{
			"lineID": 112,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Without our special wonder? You make me strange"}]
		},
		{
			"lineID": 113,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Even to the disposition that I owe"}]
		},
		{
			"lineID": 114,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 When now I think you can behold such sights"}]
		},
		{
			"lineID": 115,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And keep the natural ruby of your cheeks"}]
		},
	//------------------------------------------------------------
	//				LINES 116 - 120
	//------------------------------------------------------------
		{
			"lineID": 116,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 When mine is blanched with fear."},
	 				 {"actor": "Ross", "text": "What sights, my lord?"}]
		},
		{
			"lineID": 117,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 I pray you speak not, he grows worse and worse."}]
		},
		{
			"lineID": 118,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Question enrages him. At once, good night."}]
		},
		{
			"lineID": 119,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Stand not upon the order of your going,"}]
		},
		{
			"lineID": 120,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 But go at once."},
	 				 {"actor": "Lennox", "text": "Good night, and better health"}]
		},
	//------------------------------------------------------------
	//				LINES 121 - 125
	//------------------------------------------------------------
		{
			"lineID": 121,
			"sharedLine": true,
			"line": [{"actor": "Lennox", "text": "\u2022 Attend His Majesty!"},
	 				 {"actor": "Lady Macbeth", "text": "A kind good night to all!"}]
		},
		{
			"lineID": 122,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 It will have blood. They say blood will have blood."}]
		},
		{
			"lineID": 123,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Stones have been known to move and trees to speak."}]
		},
		{
			"lineID": 124,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Augurs and understood relations have"}]
		},
		{
			"lineID": 125,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 By maggot pies and choughs and rooks brought forth"}]
		},
	//------------------------------------------------------------
	//				LINES 126 - 130
	//------------------------------------------------------------
		{
			"lineID": 126,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 The secret'st man of blood. What is the night?"}]
		},
		{
			"lineID": 127,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 Almost at odds with morning, which is which."}]
		},
		{
			"lineID": 128,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 How say'st thou that Macduff denies his person"}]
		},
		{
			"lineID": 129,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 At our great bidding?"},
	 				 {"actor": "Lady Macbeth", "text": "Did you send to him, sir?"}]
		},
		{
			"lineID": 130,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I hear it by the way, but I will send."}]
		},
	//------------------------------------------------------------
	//				LINES 131 - 135
	//------------------------------------------------------------
		{
			"lineID": 131,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 There's not a one of them but in his house"}]
		},
		{
			"lineID": 132,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I keep a servant fee'd. I will tomorrow,"}]
		},
		{
			"lineID": 133,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And betimes I will, to the weird sisters."}]
		},
		{
			"lineID": 134,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 More shall they speak, for now I am bent to know,"}]
		},
		{
			"lineID": 135,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 By the worst means, the worst. For mine own good"}]
		},
	//------------------------------------------------------------
	//				LINES 136 - 140
	//------------------------------------------------------------
		{
			"lineID": 136,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 All causes shall give way. I am in blood"}]
		},
		{
			"lineID": 137,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Stepped in so far that should I wade no more,"}]
		},
		{
			"lineID": 138,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Returning were as tedious as go o'er."}]
		},
		{
			"lineID": 139,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Strange things I have in head that will to hand,"}]
		},
		{
			"lineID": 140,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Which must be acted ere they may be scanned."}]
		},
	//------------------------------------------------------------
	//				LINES 141
	//------------------------------------------------------------
		{
			"lineID": 141,
			"sharedLine": false,
			"line": [{"actor": "Lady Macbeth", "text": "\u2022 You lack the season of all natures, sleep."}]
		},
		{
			"lineID": 142,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Come, we'll to sleep. My strange and self-abuse"}]
		},
		{
			"lineID": 143,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Is the initiate fear that wants hard use."}]
		},
		{
			"lineID": 144,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 We are yet but young in deed."}]
		}]
}