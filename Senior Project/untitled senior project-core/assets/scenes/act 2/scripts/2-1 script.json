"script":
	[
	//------------------------------------------------------------
	//				LINES 1 - 5
	//------------------------------------------------------------
		{
			"lineID": 1,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 How goes the night, boy?"}]
		},
	 	{
	 		"lineID": 2,
	 		"sharedLine": false,
	 		"line": [{"actor": "Fleance", "text": "\u2022 The moon is down, I have not heard the clock."}]
	 	},
	 	{
	 		"lineID": 3,
	 		"sharedLine": true,
	 		"line": [{"actor": "Banquo", "text": "\u2022 And she goes down at 12."},
	 				 {"actor": "Fleance", "text": "I take 't 'tis later, sir."}]
	 	},
	 	{
	 		"lineID": 4,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Hold, take my sword. There's husbandry in Heaven,"}]
	 	},
	 	{
	 		"lineID": 5,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Their candles are all out. Take thee that too."}]
	 	},
	//------------------------------------------------------------
	//				LINES 6 - 10
	//------------------------------------------------------------
	 	{
	 		"lineID": 6,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 A heavy summons lies like lead upon me,"}]
	 	},
	 	{
	 		"lineID": 7,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 And yet I would not sleep. Merciful powers,"}]
	 	},
	 	{
	 		"lineID": 8,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Restrain me in the cursed thoughts that nature"}]
	 	},
	 	{
	 		"lineID": 9,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Gives way to in repose! Give me my sword."}]
	 	},
	 	{
	 		"lineID": 10,
	 		"sharedLine": false,
	 		"line": [{"actor": "Banquo", "text": "\u2022 Who's there?"}]
	 	},
	//------------------------------------------------------------
	//				LINES 11 - 15
	//------------------------------------------------------------
		{
			"lineID": 11,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 A friend."}]
		},
		{
			"lineID": 12,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 What, sir, not yet at rest? The King's abed."}]
		},
		{
			"lineID": 13,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 He hath been in unusual pleasure, and"}]
		},
		{
			"lineID": 14,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 Sent forth great largess to your offices."}]
		},
		{
			"lineID": 15,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 This diamond he greets your wife withal,"}]
		},
	//------------------------------------------------------------
	//				LINES 16 - 20
	//------------------------------------------------------------
		{
			"lineID": 16,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 By the name of most kind hostess, and shut up"}]
		},
		{
			"lineID": 17,
			"sharedLine": true,
			"line": [{"actor": "Banquo", "text": "\u2022 In measureless content."},
					 {"actor": "Macbeth", "text": "Being unprepared,"}]
		},
		{
			"lineID": 18,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Our will became the servant to defect,"}]
		},
		{
			"lineID": 19,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 Which else should free have wrought."},
					 {"actor": "Banquo", "text": "All's well."}]
		},
		{
			"lineID": 20,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 I dreamed last night of the three weird sisters."}]
		},
	//------------------------------------------------------------
	//				LINES 21 - 25
	//------------------------------------------------------------
		{
			"lineID": 21,
			"sharedLine": true,
			"line": [{"actor": "Banquo", "text": "\u2022 To you they have showed some truth."},
					 {"actor": "Macbeth", "text": "I think not of them."}]
		},
		{
			"lineID": 22,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Yet when we can entreat an hour to serve,"}]
		},
		{
			"lineID": 23,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 We would spend it in some words upon that business,"}]
		},
		{
			"lineID": 24,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 If you would grant the time."},
					 {"actor": "Banquo", "text": "At your kind'st leisure."}]
		},
		{
			"lineID": 25,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 If you shall cleave to my consent, when 'tis,"}]
		},
	//------------------------------------------------------------
	//				LINES 26 - 30
	//------------------------------------------------------------
		{
			"lineID": 26,
			"sharedLine": true,
			"line": [{"actor": "Macbeth", "text": "\u2022 It shall make honor for you."},
					 {"actor": "Banquo", "text": "So I lose none"}]
		},
		{
			"lineID": 27,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 In seeking to augment it, but still keep"}]
		},
		{
			"lineID": 28,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 My bosom franchised and allegiance clear,"}]
		},
		{
			"lineID": 29,
			"sharedLine": true,
			"line": [{"actor": "Banquo", "text": "\u2022 I shall be counseled."},
					 {"actor": "Macbeth", "text": "Good repose the while!"}]
		},
		{
			"lineID": 30,
			"sharedLine": false,
			"line": [{"actor": "Banquo", "text": "\u2022 Thanks, sir. The like to you!"}]
		},
	//------------------------------------------------------------
	//				LINES 31 - 35
	//------------------------------------------------------------
		{
			"lineID": 31,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Go bid thy mistress, when my drink is ready,"}]
		},
		{
			"lineID": 32,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 She strike upon the bell. Get thee to bed."}]
		},
		{
			"lineID": 33,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Is this a dagger which I see before me,"}]
		},
		{
			"lineID": 34,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 The handle toward my hand? Come, let me clutch thee."}]
		},
		{
			"lineID": 35,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I have thee not, and yet I see thee still."}]
		},
	//------------------------------------------------------------
	//				LINES 36 - 40
	//------------------------------------------------------------
		{
			"lineID": 36,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thou art not, fatal vision, sensible"}]
		},
		{
			"lineID": 37,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 To feeling as to sight? Or art thou but"}]
		},
		{
			"lineID": 38,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 A dagger of the mind, a false creation,"}]
		},
		{
			"lineID": 39,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Proceeding from the heat-oppressed brain?"}]
		},
		{
			"lineID": 40,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I see thee yet, in form as palpable"}]
		},
	//------------------------------------------------------------
	//				LINES 41 - 45
	//------------------------------------------------------------
		{
			"lineID": 41,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 As this which now I draw."}]
		},
		{
			"lineID": 42,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thou marshal'st me the way that I was going,"}]
		},
		{
			"lineID": 43,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And such an instrument I was to use."}]
		},
		{
			"lineID": 44,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Mine eyes are made the fools o' the other senses,"}]
		},
		{
			"lineID": 45,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Or else worth all the rest. I see thee still,"}]
		},
	//------------------------------------------------------------
	//				LINES 46 - 50
	//------------------------------------------------------------
		{
			"lineID": 46,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And on thy blade and dudgeon gouts of blood,"}]
		},
		{
			"lineID": 47,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Which was not so before. There's no such thing."}]
		},
		{
			"lineID": 48,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 It is the bloody business which informs"}]
		},
		{
			"lineID": 49,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thus to mine eyes. Now o'er the one half-world"}]
		},
		{
			"lineID": 50,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Nature seems dead, and wicked dreams abuse"}]
		},
	//------------------------------------------------------------
	//				LINES 51 - 55
	//------------------------------------------------------------
		{
			"lineID": 51,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 The curtained sleep. Witchcraft celebrates"}]
		},
		{
			"lineID": 52,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Pale Hecate's offerings, and withered murder,"}]
		},
		{
			"lineID": 53,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Alarumed by his sentinel, the wolf,"}]
		},
		{
			"lineID": 54,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Whose howl 's his watch, thus with his stealthy pace,"}]
		},
		{
			"lineID": 55,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 With Tarquin's ravishing strides, toward his design"}]
		},
	//------------------------------------------------------------
	//				LINES 56 - 60
	//------------------------------------------------------------
		{
			"lineID": 56,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Moves like a ghost. Thou sure and firm-set earth,"}]
		},
		{
			"lineID": 57,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Hear not my steps, which way they walk, for fear"}]
		},
		{
			"lineID": 58,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Thy very stones prate of my whereabout,"}]
		},
		{
			"lineID": 59,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 And take the present horror from the time,"}]
		},
		{
			"lineID": 60,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Which now suits with it. Whiles I threat, he lives."}]
		},
	//------------------------------------------------------------
	//				LINES 61 - 64
	//------------------------------------------------------------
		{
			"lineID": 61,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Words to the heat of deeds too cold breath gives."}]
		},
		{
			"lineID": 62,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 I go, and it is done. The bell invites me."}]
		},
		{
			"lineID": 63,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 Hear it not, Duncan, for it is a knell"}]
		},
		{
			"lineID": 64,
			"sharedLine": false,
			"line": [{"actor": "Macbeth", "text": "\u2022 That summons thee to Heaven, or to Hell."}]
		}
	 ]
}