	[
		"SCENE I. A desert place.",
		"Thunder and lightning. Enter three Witches",
		"",
		"First Witch",
			"\tWhen shall we three meet again",
			"\tIn thunder, lightning, or in rain?",
		"",
		"Second Witch",
			"\tWhen the hurlyburly's done,",
			"\tWhen the battle's lost and won.",
		"",
		"Third Witch",
			"\tThat will be ere the set of sun.",
		"",
		"First Witch",
			"\tWhere the place?",
		"",
		"Second Witch",
			"\tUpon the heath.",
		"",
		"Third Witch",
			"\tThere to meet with Macbeth.",
		"",
		"First Witch",
			"\tI come, Graymalkin!",
		"",
		"Second Witch",
			"\tPaddock calls.",
		"",
		"Third Witch",
			"\tAnon.",
		"",
		"ALL",
			"\tFair is foul, and foul is fair:",
			"\tHover through the fog and filthy air.",
		"",
		"Exeunt"
	]
}