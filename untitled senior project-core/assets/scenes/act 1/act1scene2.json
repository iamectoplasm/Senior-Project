{
"cast": ["Duncan", "Malcolm", "Donalbain",
		 "Lennox", "Ross", "Sergeant"],

"script":
	[
		{
			"lineID": 1,
			"sharedLine": false,
			"line": [{"actor": "Duncan", "text": "-What bloody man is that? He can report,"}]
		},
	 	{
	 		"lineID": 2,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-As seemeth by his plight, of the revolt"}]
	 	},
	 	{
	 		"lineID": 3,
	 		"sharedLine": true,
	 		"line": [{"actor": "Duncan", "text": "-The newest state."},
	 				 {"actor": "Malcolm", "text": "-This is the Sergeant"}]
	 	},
	 	{
	 		"lineID": 4,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "-Who like a good and hardy soldier fought"}]
	 	},
	 	{
	 		"lineID": 5,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "-'Gainst my captivity. Hail, brave friend!"}]
	 	},
	 	{
	 		"lineID": 6,
	 		"sharedLine": false,
	 		"line": [{"actor": "Malcolm", "text": "-Say to the King the knowledge of the broil"}]
	 	},
	 	{
	 		"lineID": 7,
	 		"sharedLine": true,
	 		"line": [{"actor": "Malcolm", "text": "-As thou didst leave it."},
	 				 {"actor": "Sergeant", "text": "Doubtful it stood,"}]
	 	},
	 	{
	 		"lineID": 8,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-As two spent swimmers that do cling together"}]
	 	},
	 	{
	 		"lineID": 9,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-And choke their art. The merciless Macdonwald--"}]
	 	},
	 	{
	 		"lineID": 10,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Worthy to be a rebel, for to that"}]
	 	},
	 	{
	 		"lineID": 11,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-The multiplying villainies of nature"}]
	 	},
	 	{
	 		"lineID": 12,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Do swarm upon him-- from the western isles"}]
	 	},
	 	{
	 		"lineID": 13,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Of kerns and gallowglasses is supplied."}]
	 	},
	 	{
	 		"lineID": 14,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-And fortune, on his damnèd quarrel smiling"}]
	 	},
	 	{
	 		"lineID": 15,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Showed like a rebel's whore. But all's too weak."}]
	 	},
	 	{
	 		"lineID": 16,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-For brave Macbeth-- well he deserves that name--"}]
	 	},
	 	{
	 		"lineID": 17,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Disdaining fortune, with his brandished steel,"}]
	 	},
	 	{
	 		"lineID": 18,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Which smoked with bloody execution,"}]
	 	},
	 	{
	 		"lineID": 19,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Like valor's minion carvèd out his passage"}]
	 	},
	 	{
	 		"lineID": 20,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Till he faced the slave,"}]
	 	},
	 	{
	 		"lineID": 21,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Which ne'er shook hands, nor bade farewell to him,"}]
	 	},
	 	{
	 		"lineID": 22,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Till he unseamed him from the nave to his chaps,"}]
	 	},
	 	{
	 		"lineID": 23,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-And fixed his head upon our battlements."}]
	 	},
	 	{
	 		"lineID": 24,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Oh, valiant cousin! Worthy gentleman!"}]
	 	},
	 	{
	 		"lineID": 25,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-As whence the sun 'gins his reflection"}]
	 	},
	 	{
	 		"lineID": 26,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Shipwrecking storms and direful thunders break,"}]
	 	},
	 	{
	 		"lineID": 27,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-So from that spring whence comfort seemed to come"}]
	 	},
	 	{
	 		"lineID": 28,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Discomfort swells. Mark, King of Scotland, mark."}]
	 	},
	 	{
	 		"lineID": 29,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-No sooner justice had, with valor armed,"}]
	 	},
	 	{
	 		"lineID": 30,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Compelled these skipping kerns to trust their heels,"}]
	 	},
	 	{
	 		"lineID": 31,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-But the Norweyan lord, surveying vantage,"}]
	 	},
	 	{
	 		"lineID": 32,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-With furbished arms and new supplies of men,"}]
	 	},
	 	{
	 		"lineID": 33,
	 		"sharedLine": true,
	 		"line": [{"actor": "Sergeant", "text": "-Began a fresh assault"},
	 				 {"actor": "Duncan", "text": "Dismayed not this"}]
	 	},
	 	{
	 		"lineID": 34,
	 		"sharedLine": true,
	 		"line": [{"actor": "Duncan", "text": "-Our captains, Macbeth and Banquo?"},
	 				 {"actor": "Sergeant", "text": "Yes,"}]
	 	},
	 	{
	 		"lineID": 35,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-As sparrows eagles, or the hare the lion."}]
	 	},
	 	{
	 		"lineID": 36,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-If I say sooth, I must report they were"}]
	 	},
	 	{
	 		"lineID": 37,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-As cannons overcharged with double cracks,"}]
	 	},
	 	{
	 		"lineID": 38,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-So they doubly redoubled strokes upon the foe"}]
	 	},
	 	{
	 		"lineID": 39,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Except they meant to bathe in reeking wounds,"}]
	 	},
	 	{
	 		"lineID": 40,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-Or memorize another Golgotha,"}]
	 	},
	 	{
	 		"lineID": 41,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-I cannot tell--"}]
	 	},
	 	{
	 		"lineID": 42,
	 		"sharedLine": false,
	 		"line": [{"actor": "Sergeant", "text": "-But I am faint, my gashes cry for help."}]
	 	},
	 	{
	 		"lineID": 43,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-So well thy words become thee as thy wounds."}]
	 	},
	 	{
	 		"lineID": 44,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-They smack of honor both. Go get him surgeons."}]
	 	},
	 	{
	 		"lineID": 45,
	 		"sharedLine": true,
	 		"line": [{"actor": "Duncan", "text": "-Who comes here?"},
	 				 {"actor": "Malcolm", "text": "The worthy Thane of Ross."}]
	 	},
	 	{
	 		"lineID": 46,
	 		"sharedLine": false,
	 		"line": [{"actor": "Lennox", "text": "-What a haste looks through his eyes! So should he look"}]
	 	},
	 	{
	 		"lineID": 47,
	 		"sharedLine": true,
	 		"line": [{"actor": "Lennox", "text": "-That seems to speak things strange."},
	 				 {"actor": "Ross", "text": "God save the King!"}]
	 	},
	 	{
	 		"lineID": 48,
	 		"sharedLine": true,
	 		"line": [{"actor": "Duncan", "text": "-Whence camest thou, worthy Thane?"},
	 				 {"actor": "Ross", "text": "From Fife, great King,"}]
	 	},
	 	{
	 		"lineID": 49,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Where the Norweyan banners flout the sky"}]
	 	},
	 	{
	 		"lineID": 50,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-And fan our people cold."}]
	 	},
	 	{
	 		"lineID": 51,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Norway himself, with terrible numbers,"}]
	 	},
	 	{
	 		"lineID": 52,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Assisted by that most disloyal traitor"}]
	 	},
	 	{
	 		"lineID": 53,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-The Thane of Cawdor, began a dismal conflict,"}]
	 	},
	 	{
	 		"lineID": 54,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Till that Bellona's bridegroom, lapped in proof,"}]
	 	},
	 	{
	 		"lineID": 55,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Confronted him with self-comparisons,"}]
	 	},
	 	{
	 		"lineID": 56,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Point against point rebellious, arm 'gainst arm,"}]
	 	},
	 	{
	 		"lineID": 57,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Curbing his lavish spirit. And, to conclude,"}]
	 	},
	 	{
	 		"lineID": 58,
	 		"sharedLine": true,
	 		"line": [{"actor": "Ross", "text": "-The victory fell on us."},
	 				 {"actor": "Duncan", "text": "Great happiness!"},
	 				 {"actor": "Ross", "text": "That now"}]
	 	},
	 	{
	 		"lineID": 59,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Sweno, the Norway's king, craves composition"}]
	 	},
	 	{
	 		"lineID": 60,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Nor would we deign him burial of his men"}]
	 	},
	 	{
	 		"lineID": 61,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Till he disbursèd, at Saint Colme's inch,"}]
	 	},
	 	{
	 		"lineID": 62,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-Ten thousand dollars to our general use."}]
	 	},
	 	{
	 		"lineID": 63,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-No more that Thane of Cawdor shall deceive"}]
	 	},
	 	{
	 		"lineID": 64,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-Our bosom interest. Go pronounce his present death,"}]
	 	},
	 	{
	 		"lineID": 65,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-And with his former title greet Macbeth."}]
	 	},
	 	{
	 		"lineID": 66,
	 		"sharedLine": false,
	 		"line": [{"actor": "Ross", "text": "-I'll see it done."}]
	 	},
	 	{
	 		"lineID": 67,
	 		"sharedLine": false,
	 		"line": [{"actor": "Duncan", "text": "-What he hath lost noble Macbeth hath won."}]
	 	}]
}